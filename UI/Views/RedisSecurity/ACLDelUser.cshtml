@using Core
@model RedisACLDelUserViewModel

<div class="card-dark mt-4">
        <h3>ACL DELUSER</h3>
        <form method="post" class="row g-3">
            <div class="col-md-4">
                <label>User Name</label>
                <input asp-for="UserName" class="form-control bg-secondary text-light" />
            </div>
            <div class="col-12 mt-2">
