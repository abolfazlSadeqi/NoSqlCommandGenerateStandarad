@using Core
@model RedisRequirePassViewModel
<div class="card-dark mt-4">
        <h3>Set requirepass</h3>
        <form method="post" class="row g-3">
            <div class="col-md-6">
                <label>Password</label>
                <input asp-for="Password" class="form-control bg-secondary text-light" type="password" />
            </div>
            <div class="col-12 mt-2">
                <button class="btn btn-info">Generate Command</button>
            </div>
        </form>

        @await Html.PartialAsync("_ScriptResult", Model)
    
</div>
@section Scripts {
    <partial name="_ValidationScriptsPartial" />


}